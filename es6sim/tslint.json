{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [true, "never-prefix"],
        "adjacent-overload-signatures": true,
        "align": [true, "parameters"],
        "curly": [true, "ignore-same-line"],
        "await-promise": [true, "Thenable"],
        "brace-style": [true, "1tbs", {"allowSingleLine": true}],
        "callable-types": false,
        "class-name": true,
        "comment-format": [true, "check-space"],
        "eofline": true,
        "import-blacklist": [true, "rxjs"],
        "import-spacing": true,
        "indent": [true, "spaces"],
        "interface-over-type-literal": true,
        "label-position": true,
        "max-line-length": [true, 160],
        "member-access": false,
        "no-arg": true,
        "no-bitwise": true,
        "no-console": [true, "debug", "info", "time", "timeEnd", "trace"],
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": true,
        "no-construct": true,
        "no-control-regex": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": false,
        "no-empty-character-class": true,
        "no-empty-interface": false,
        "no-eval": true,
        "no-extra-boolean-cast": true,
        "no-extra-semi": true,
        "no-inferrable-types": [true, "ignore-params"],
        "no-inner-declarations": [true, "both"],
        "no-internal-module": true,
        "no-input-rename": false,
        "no-invalid-regexp": true,
        "no-invalid-this": [true, "check-function-in-method"],
        "no-irregular-whitespace": true,
        "no-misused-new": true,
        "no-multiple-var-decl": true,
        "no-namespace": true,
        "no-non-null-assertion": false,
        "no-null-keyword": true,
        "no-octal-literal": true,
        "no-reference": true,
        "no-regex-spaces": true,
        "no-require-imports": true,
        "no-shadowed-variable": true,
        "no-sparse-arrays": true,
        "no-string-literal": false,
        "no-string-throw": true,
        "no-switch-case-fall-through": true,
        "no-trailing-whitespace": true,
        "no-unused-expression": true,
        "no-use-before-declare": false,
        "no-var-keyword": true,
        "no-var-requires": true,
        "no-with-statement": true,
        "object-literal-key-quotes": [true, "as-needed"],
        "one-line": [true, "check-open-brace", "check-catch", "check-else", "check-whitespace"],
        "one-variable-per-declaration": true,
        "ordered-imports": true,
        "prefer-array-literal": true,
        "prefer-const": [
        true,
        {
            "destructuring": "all"
        }
        ],
        "prefer-for-of": true,
        "promise-function-async": false,
        "quotemark": [true, "single", "avoid-escape"],
        "semicolon": [true, "never"],
        "space-before-function-paren": [true, "always"],
        "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
        "triple-equals": [true, "allow-null-check"],
        "typedef": [true, "property-declaration"],
        "typedef-whitespace": [true, {
            "call-signature": "nospace",
            "index-signature": "nospace",
            "parameter": "nospace",
            "property-declaration": "nospace",
            "variable-declaration": "nospace"
        }
        ],
        "typeof-compare": true,
        "unified-signatures": false,
        "valid-typeof": true,
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
        "whitespace": [true, "check-branch", "check-decl", "check-operator", "check-module", "check-separator", "check-type", "check-typecast", "check-preblock"]
    },
    "rulesDirectory": [],
    "autoFixOnSave ": true
}